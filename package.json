{
  "name": "currietechnologies-razorpagevisibility",
  "version": "1.0.0",
  "description": "A Razor class library for interacting with the browser visibility API",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p --color",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/*.ts",
    "prettier:check": "prettier --check ./src/*.ts",
    "babel": "babel ./src/pagevisibility.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.3.5",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": [
    "ie 10",
    "edge 12",
    "chrome 33",
    "firefox 18",
    "safari 6.1",
    "ios_saf 7",
    "opera 12.1",
    "and_chr 76",
    "and_ff 68",
    "and_uc 12.12",
    "samsung 5",
    "and_qq 1.2"
  ],
  "author": "Michael J. Currie"
}
